#define XAP_RID_DIALOG_LIST_DOCUMENTS_TITLE	1001
#define XAP_RID_DIALOG_LIST_DOCUMENTS_HEADING    1002
#define XAP_RID_DIALOG_LIST_DOCUMENTS_LIST       1003
#define XAP_RID_DIALOG_LIST_DOCUMENTS_BTN_OK  	IDOK
#define XAP_RID_DIALOG_LIST_DOCUMENTS_BTN_CANCEL	IDCANCEL

#ifdef RC_INVOKED
// NOTE -- these placeholder strings get overridden at runtime
// NOTE -- they're just here to make sizing easier

XAP_RID_DIALOG_LIST_DOCUMENTS DIALOG DISCARDABLE  0, 0, 300, 151
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Dialog"
FONT 8, "MS Shell Dlg 2"
BEGIN
    DEFPUSHBUTTON   "Activate",IDOK,243,22,50,14
    PUSHBUTTON      "Close",IDCANCEL,243,41,50,14
//  GROUPBOX        "Open Documents",XAP_RID_DIALOG_LIST_DOCUMENTS_FRAME,7,7,232,137
	LTEXT			"Choose document:", XAP_RID_DIALOG_LIST_DOCUMENTS_HEADING,
					7,7,225,10, SS_CENTERIMAGE

    CONTROL         "List1",XAP_RID_DIALOG_LIST_DOCUMENTS_LIST,"SysListView32",LVS_REPORT | 
                    LVS_SINGLESEL | LVS_AUTOARRANGE | WS_BORDER | WS_TABSTOP,
                    7,22,230,122
END
#endif /* RC_INVOKED */
